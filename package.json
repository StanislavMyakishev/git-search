{
  "name": "sber-test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@types/history": "^4.7.6",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.0",
    "apollo-boost": "^0.4.9",
    "graphql": "^15.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "styled-components": "^5.1.1",
    "typescript": "~3.7.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.15.3",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "eslint": "^6.8.0",
    "eslint-config-preact": "^1.1.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-decorators": "0.0.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "history": "^4.10.1",
    "husky": "^4.2.3",
    "prettier": "^2.0.5",
    "react-router-dom": "^5.1.2",
    "@graphql-codegen/typescript": "1.15.3",
    "@graphql-codegen/typescript-operations": "1.15.3",
    "@graphql-codegen/typescript-react-apollo": "1.15.3",
    "@graphql-codegen/introspection": "1.15.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && npm run lint-staged && git add ."
    }
  },
  "lint-staged": {
    "*.{ts, tsx}": [
      "prettier --write",
      "npm run lint-staged",
      "git add ."
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "precommit": "npm run prettier && npm run lint-staged && git add .",
    "prettier": "prettier --write '*/**/*.{ts,tsx,js}'",
    "lint-staged": "tsc --noEmit && eslint '*/**/*.{ts,tsx,js}' --max-warnings=0 --fix",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
